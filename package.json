{
  "name": "transbank-sdk",
  "version": "1.0.0",
  "description": "Official SDK from Transbank Webpay",
  "main": "./index.js",
  "types": "lib/webpay/soap/webpay.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "keywords": [
    "transbank",
    "webpay",
    "pagos",
    "payments"
  ],
  "homepage": "https://transbankdevelopers.cl",
  "dependencies": {
    "node-uuid": "^1.4.7",
    "pem": "^1.9.4",
    "soap": "^0.18.0",
    "ursa-purejs": "0.0.3",
    "xmldom": "^0.1.27",
    "xml-crypto": "^0.8.5"
  },
  "devDependencies": {
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "prettier": "^1.13.2",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Gonzalo De Sp√≠rito <gonzalo.despirito@continuum.cl> (https://transbankdevelopers.cl)",
  "license": "BSD-3 with Clause"
}
